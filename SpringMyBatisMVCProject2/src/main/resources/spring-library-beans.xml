<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans 
       http://www.springframework.org/schema/beans/spring-beans.xsd
       ">
       
       <!-- controller -->
       <bean class="controller.library.LibraryController" />
       
       <!-- service -->
       <bean id ="libraryBoardService"  
       		class="service.libraryBoard.LibraryBoardService" />
       <bean id ="libraryBoardListService"  
       		class="service.libraryBoard.LibraryBoardListService" />
       	<bean id ="libraryBoardDetailService"  
       		class="service.libraryBoard.LibraryBoardDetailService" />	
       	<bean id ="libBoardModifyPro"  
       		class="service.libraryBoard.LibBoardModifyPro" />
       	<bean id ="fileDelService"  
       		class="service.libraryBoard.FileDelService" />
       		
       <!-- repository -->	
       <bean id="libraryBoardRepository" 
       		class="repository.LibraryBoardRepository" />
       		
</beans> 